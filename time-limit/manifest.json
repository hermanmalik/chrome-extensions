{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "html/popup.html",
      "default_icon": "images/icon.png"
    },
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://youtube.com/*",
          "https://*.youtube.com/*",
          "https://reddit.com/*",
          "https://*.reddit.com/*",
          "https://hermanmalik.com/*"
        ]
      }
    ]
}